body,html{
	padding:0;
}
#navUl > li.active > a{
	color:#1ab394;
}
#divmap{
	height:calc(100% - 30px);
	padding:14px 20px 0px 20px;
	
}
#chooseMap{
	width:200px;
	float:left;
	margin-right:24px;
	border:1px solid #d1d8e6;
}
#chooseMap li{
	height:72px;
	line-height:72px;
	border-bottom:1px solid #d1d8e6;
	text-align:center;
}
#chooseMap li .div1{
	width:66px;
	height:100%;
	float:left;
	border-right:1px solid #d1d8e6;
}
#chooseMap li .div1 label{
	display:block;
	width:100%;
	height:100%;
}
#chooseMap li .div2{
	height:100%;
	overflow:hidden;
}
#mapMain{
	height:calc(100% - 50px);
	border:1px solid #d1d8e6;
	overflow:hidden;
}
#map{
	width:970px;
	margin:0 auto;
	height:100%;
	background:#000;
}
#prjTable{
	text-align:center;
}
#prjTable td{
	border:1px solid #d1d8e6;
	font-size:14px;
}
.w130{
	width:130px;
}
.w110{
	width:110px;
}
.w60{
	width:60px;
}

